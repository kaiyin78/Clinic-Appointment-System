@model IEnumerable<Doctor>
@{
    ViewBag.Title = "Doctor";

}

<h2>Doctor</h2>
<table class="table">
    <tr>
        <th>Doctor ID</th>
        <th>Doctor Name</th>
        <th>Doctor Email</th>
        <th>Gender</th>

        <th></th>
    </tr>
    @foreach (var s in Model)
    {
<tr>
    <td>@s.Username</td>
    <td>@s.Name</td>
    <td>@s.Email</td>
    <td>@s.Gender</td>


</tr>
    }
    @Html.ActionLink("Make Appointment", "Appointment")
</table>
